@model Assignment
@{
    ViewBag.Title = "Assignment Details";
}

<h1>Student Details</h1>
<div class="row">
    <div class="col-sm-8">
        <table class="table table-bordered table-striped">
            <tr>
                <td>Assignment Name:</td>
                <td>@Model.AssignmentName</td>
            </tr>
            <tr>
                <td>GitHub Url:</td>
                <td>@Model.GitHubUrl</td>
            </tr>
        </table>
    </div>
</div>